
public class FAT {
	EntradaFat [] entradasFat;
	Cluster [] clusters;
	
	public FAT()
	{
		EntradaFat [] entradasFat = new  EntradaFat[20];
		Cluster [] clusters = new Cluster[20];
	}
	
	public void mostrarMetadatos()
	{
		for(int i = 0; i < 20; i++)
		{
			System.out.println("Cluster " + i + ":");
			entradasFat[i].mostrar();
		}
	}
	
	
	
}
