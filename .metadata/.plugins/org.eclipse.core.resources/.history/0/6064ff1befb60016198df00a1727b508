
public class EntradaFat {

	private	boolean disponible;
	private	boolean fin;
	private	int siguiente;
	
	public EntradaFat()
	{
		disponible = true;
		fin = false;
		siguiente = -1;
	}
	
	public void mostrar()
	{
		System.out.println("Disponible = " + disponible);
		System.out.println("Siguiente = " + siguiente);
		System.out.println("Fin  = " + fin);
	}
	
	public boolean getDispoible()
	{
		return disponible;
	}
	
	public boolean getFin()
	{
		return fin;
	}
	
	public int getSiguiente()
	{
		return siguiente;
	}
	
	public void setDisponible(boolean cambio)
	{
		disponible = cambio;
	}
	
	public void setFin(boolean cambio)
	{
		fin = cambio;
	}
	
	public void setSiguiente(int numCluster)
	{
		siguiente = numCluster;
	}
	
	
		
}
